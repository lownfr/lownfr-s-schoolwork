for i=0 to 2 do
    
    BEGIN
    j:=0
    
    [i,j]:=loc0
    j:=j+1
    [i,j]:=loc1
    j:=j+1
    [i,j]:=loc2
    
    if (loc0=loc1) AND (loc1=loc2) then     win
    
    END
    
    
    
